<?xml version="1.0" encoding="utf-8"?>
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
id="com.telefonica.movistar.plugin"
version="0.7.0">
<name>NetworkInformation</name>
<engines>
<engine name="cordova" version=">=3.4.0"/>
</engines>
<asset src="www/netinfo.js" target="js/netinfo.js"/>
<js-module src="www/netinfo.js" name="hello">
<clobbers target="netinfo" />
</js-module>
<platform name="android">
<config-file target="res/xml/config.xml" parent="/*">
<feature name="Hello">
<param name="android-package" value="com.telefonica.movistar.plugin."/>
</feature>
</config-file>
<source-file src="src/android/Hello.java" target-dir="src/com/telefonica/movistar/plugin/"/>
</platform>
</plugin>
